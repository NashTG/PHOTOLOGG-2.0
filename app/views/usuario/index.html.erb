<% @usuario.each do |usuario| %>
      <tr>
        <li><%= usuario.NICK %>, <%= usuario.CORREO %>
          <% if (current_user.TIPO_USUARIO == 2) %>
        |   <%= link_to 'Eliminar', usuario, method: :delete, data: { confirm: '¿Estas seguro?' } %>
          <%else%>
        |    <%= link_to 'Agregar Amigo', añadir_amigos %>
          <% end%>
        </li>

      </tr>
    <% end %>
